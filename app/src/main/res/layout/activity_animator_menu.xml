<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".part1.uniteanimatorxml10.AnimatorMenuActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_menu_item_0"
        style="@style/Custom.MenuItemStyle"
        android:background="@drawable/icon_menu_0"
        android:visibility="gone"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_menu_item_1"
        style="@style/Custom.MenuItemStyle"
        android:background="@drawable/icon_menu_1"
        android:visibility="gone"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_menu_item_2"
        style="@style/Custom.MenuItemStyle"
        android:background="@drawable/icon_menu_2"
        android:visibility="gone"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_menu_item_3"
        style="@style/Custom.MenuItemStyle"
        android:background="@drawable/icon_menu_3"
        android:visibility="gone"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_menu_item_4"
        style="@style/Custom.MenuItemStyle"
        android:background="@drawable/icon_menu_4"
        android:visibility="gone"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_menu"
        style="@style/Custom.MenuStyle"
        android:layout_marginEnd="16dp"
        android:background="@android:drawable/ic_dialog_email"
        app:layout_goneMarginEnd="16dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>